  <info>
    
    <link xref="GstPbutils.EncodingTarget" group="function" type="guide"/>
    <api:function>
      <api:returns>
        <api:type>GstPbutils.EncodingTarget</api:type>
      </api:returns>
      <api:name>gst_encoding_target_load</api:name>
      <api:arg>
        <api:type>String</api:type>
        <api:name>name</api:name>
      </api:arg>
      <api:arg>
        <api:type>String</api:type>
